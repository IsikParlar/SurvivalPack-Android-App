<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.survivalpack">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/survivalpackicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/survivalpackicon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SurvivalPack">
        <activity
            android:name=".activity.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".activity.OzelCantaActivity"
            android:exported="false" />
        <activity
            android:name=".activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".activity.KampCantasiActivity"
            android:exported="false" />
        <activity
            android:name=".activity.DagciCantasiActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CocukCantasiActivity"
            android:exported="false" />
        <activity
            android:name=".activity.IlkYardimCantasiActivity"
            android:exported="false" />
        <activity
            android:name=".activity.DepremCantasiActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CantamActivity"
            android:exported="false" />
        <activity
            android:name=".activity.FavoriActivity"
            android:exported="false" />
        <activity android:name=".activity.AnaEkranActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>